define("article:widget/left-promo/left-promo.js",function(e,t,o){var i=e("common-jquery:widget/lib/jquery/jquery.js"),n=e("common-jquery:widget/lib/cookie/cookie.js"),r=e("common-jquery:widget/js/logic/log/log.js"),c={target:i(".wgt-left-promo"),init:function(){var e=this;e.resize(),e.eventHandler(),r.send(location.href,2019,{module:"article",page:"index",pos:"left-promo",action:"show"})},eventHandler:function(){var e=this;e.lock=!1,i(window).on("resize",function(){e.lock||(e.lock=!0,setTimeout(function(){e.resize(),e.lock=!1},300))}),i("#close-left-promo").click(function(t){t&&t.preventDefault?t.preventDefault():window.event.returnValue=!1,n.set("EXP_LEFT_PROMO",1,{expires:432e5}),e.target.hide()}),i("#wgt-left-promo").on("click",function(){r.send(location.href,2019,{module:"article",page:"index",pos:"left-promo",action:"click"})})},resize:function(){var e=this;if(n.get("EXP_LEFT_PROMO"))return void e.target.hide();var t=i(window).height(),o=e.target.height(),r=e.target.width(),c=i("#wgt-like"),l=.9*t-o,s=i(".content-container"),a=s.offset().left-r-5;if(c.size()>0&&"none"!==c.css("display")){var f=parseInt(c.css("top"),10);l=f-o}e.target.css({top:l,left:a}).removeClass("hide")}};o.exports=c});