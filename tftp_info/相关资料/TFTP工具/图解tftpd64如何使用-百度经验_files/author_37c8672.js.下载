define("article:widget/author/author.js",function(o,n,t){var e=o("common-jquery:widget/lib/jquery/jquery.js"),l=o("common-jquery:widget/js/ui/follow/follow.es"),i=e(".usr-explist li");i.length&&e.each(i,function(o,n){e(n).on("mouseover",function(){e(n).addClass("on")}),e(n).on("mouseout",function(){e(n).removeClass("on")})});var r=function(o){var n=e(o);n.removeClass("follow-btn"),n.addClass("followed-btn")};t.exports.initFollow=function(o){l.init({followId:o,uname:F.context("writerId"),uid:F.context("wirterUidEnc"),followCallback:r,follow:l,BdStoken:F.context("BdStoken"),bdstt:F.context("bdstt")}),e(".crown").on("click",function(o){o.preventDefault(),window.open("/shop/view?id=1384","_blank")})}});