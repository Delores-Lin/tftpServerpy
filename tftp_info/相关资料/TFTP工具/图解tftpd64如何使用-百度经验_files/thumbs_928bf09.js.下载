define("article:widget/thumbs/thumbs.js",function(e,t,o){var i=e("common-jquery:widget/lib/jquery/jquery.js");o.exports.init=function(){var e=null,t=!1,o=i(".wgt-thumbs .zoom-wrap");i.each(o,function(o,n){i(n).on("mouseenter",function(o){o.preventDefault(),o.stopPropagation(),e=setTimeout(function(){t=!0,i(n).parent("li").addClass("zoom-wrap-hover"),i("img",n).eq(0).animate({width:92,height:69},200),i(n).animate({top:-9.5,left:-12.5},200)},100)}),i(n).on("mouseleave",function(o){o.preventDefault(),o.stopPropagation(),clearTimeout(e),t&&(t=!1,i(n).parent("li").removeClass("zoom-wrap-hover"),i("img",n).eq(0).animate({width:67,height:50},200),i(n).animate({top:0,left:0},200))})})}});